import Vue from 'vue'
import VueRouter from 'vue-router'
import Home from '../views/Home.vue'
{{#each list}}
import {{componentName}} from '../views/{{file}}'
{{/each}}
Vue.use(VueRouter)

const routes = [{
    path: '/',
    name: 'home',
    component: Home
  },
  {{#each list}}
  {
    path: '/{{name}}',
    name: '{{name}}',
    component: {{componentName}}
  },
  {{/each}}
]

const router = new VueRouter({
  mode: 'history',
  base: process.env.BASE_URL,
  routes
})

export default router